{
  "name": "tabell",
  "description": "Tabell",
  "main": "lib/index.js",
  "version": "1.1.9",
  "license": "MIT",
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.0"
  },
  "scripts": {
    "build": " npm-run-all -p build:*",
    "build:css": "less-watch-compiler --run-once --config less.json",
    "dist": "rm -rf ./lib && webpack --config webpack.prod.js",
    "dist:dev": "rm -rf ./lib && webpack --config webpack.dev.js",
    "lint": "standardx src/*.ts* src/*/*.ts*",
    "lint:fix": "standardx src/*.ts* src/*/*.ts* --fix",
    "npx": "npx npm-force-resolutions",
    "start": "npm-run-all -p start:*",
    "start:js": "react-scripts start",
    "start:css": "less-watch-compiler --config less.json",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "react-scripts test --coverage  --watchAll=false"
  },
  "homepage": ".",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@navikt/fnrvalidator": "^1.1.3",
    "classnames": "^2.2.6",
    "jest-styled-components": "^7.0.3",
    "lodash": "^4.17.20",
    "lodash.throttle": "^4.1.1",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "nav-frontend-ikoner-assets": "^2.0.8",
    "nav-frontend-lenker": "^1.0.46",
    "nav-frontend-lenker-style": "^0.2.33",
    "nav-frontend-paneler-style": "^0.3.25",
    "nav-frontend-skjema": "^3.0.27",
    "nav-frontend-skjema-style": "^2.0.16",
    "nav-frontend-spinner": "^2.0.10",
    "nav-frontend-spinner-style": "^0.2.8",
    "nav-frontend-tabell-style": "^0.2.27",
    "nav-frontend-typografi": "^2.0.31",
    "nav-frontend-typografi-style": "^1.0.26",
    "nav-styled-component-theme": "^1.0.7",
    "paginering": "^1.0.6",
    "prop-types": "^15.7.2",
    "rc-tooltip": "^4.2.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.1.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.162",
    "@types/md5": "^2.2.1",
    "@types/prop-types": "^15.7.3",
    "@types/rc-tooltip": "^3.7.3",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.4",
    "copy-webpack-plugin": "^6.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "less-plugin-npm-import": "^2.1.0",
    "less-watch-compiler": "^1.14.6",
    "npm-run-all": "^4.1.5",
    "react-scripts": "^4.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.0",
    "standardx": "^5.0.0",
    "svg-url-loader": "^6.0.0",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "webpack-cli": "^4.1.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "resolutions": {
    "yargs-parser": "18.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/tabell.git"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "globals": [
      "Blob",
      "File",
      "XMLHttpRequest",
      "HTMLCanvasElement",
      "FileReader",
      "ReactDOM",
      "WebSocket",
      "act",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest",
      "JSX"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "ignore": [
      "build/**/*.js",
      "public/**/*.js",
      "node_modules/**/*.js",
      "build/**/*.tsx",
      "public/**/*.tsx",
      "node_modules/**/*.tsx",
      "src/**/*.snap"
    ]
  }
}
