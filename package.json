{
  "name": "@navikt/tabell",
  "description": "Tabell",
  "main": "lib/index.js",
  "version": "4.1.1",
  "license": "MIT",
  "homepage": ".",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "react-scripts build",
    "dist": "npm run dist:prod",
    "dist:prod": "rm -rf ./lib && webpack --config webpack.prod.js",
    "dist:dev": "rm -rf ./lib && webpack --config webpack.dev.js",
    "lint": "standardx 'src/*.ts*'",
    "lint:fix": "standardx 'src/*.ts*' --fix",
    "npx": "npx npm-force-resolutions",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "react-scripts test --coverage  --watchAll=false"
  },
  "peerDependencies": {
    "@navikt/ds-css": "^6.x",
    "@navikt/ds-react": "^6.x",
    "@navikt/hoykontrast": "^4.0.0",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@navikt/ds-css": "^6.x",
    "@navikt/ds-react": "^6.x",
    "@navikt/fnrvalidator": "^1.3.0",
    "@navikt/hoykontrast": "^4.0.0",
    "@navikt/tooltip": "^1.1.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/classnames": "^2.3.0",
    "@types/jest": "^28.1.7",
    "@types/lodash": "^4.14.184",
    "@types/md5": "^2.3.2",
    "@types/mustache": "^4.2.1",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-syntax-highlighter": "^15.5.4",
    "@types/styled-components": "^5.1.26",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^11.0.0",
    "inspectpack": "^4.7.1",
    "jest-styled-components": "^7.1.0",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "mustache": "^4.2.0",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-normalize": "^10.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "standardx": "^7.0.0",
    "styled-components": "^5.3.5",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack-cli": "^4.10.0",
    "webpack-stats-plugin": "^1.1.0"
  },
  "resolutions": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/tabell.git"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "globals": [
      "Blob",
      "File",
      "XMLHttpRequest",
      "HTMLButtonElement",
      "HTMLCanvasElement",
      "HTMLElement",
      "HTMLInputElement",
      "FileReader",
      "ReactDOM",
      "WebSocket",
      "act",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest",
      "JSX"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "ignore": [
      "build/**/*.js",
      "public/**/*.js",
      "node_modules/**/*.js",
      "build/**/*.tsx",
      "public/**/*.tsx",
      "node_modules/**/*.tsx",
      "src/**/*.snap"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
