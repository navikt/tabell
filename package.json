    "@types/styled-components": "^5.1.23",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "classnames": "^2.3.1",
    "copy-webpack-plugin": "^10.2.4",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "inspectpack": "^4.7.1",
    "jest-styled-components": "^7.0.8",
    "md5": "^2.3.0",
