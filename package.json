    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "dependencies": {
    "@navikt/paginering": "^3.0.0"
  },
  "devDependencies": {
    "@navikt/ds-css": "^0.x",
    "@navikt/ds-icons": "^0.x",
