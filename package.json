{
  "name": "@navikt/tabell",
  "description": "Tabell",
  "main": "lib/index.js",
  "version": "3.0.6",
  "license": "MIT",
  "homepage": ".",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "react-scripts build",
    "dist": "npm run dist:prod",
    "dist:prod": "rm -rf ./lib && webpack --config webpack.prod.js",
    "dist:dev": "rm -rf ./lib && webpack --config webpack.dev.js",
    "lint": "standardx 'src/*.ts*'",
    "lint:fix": "standardx 'src/*.ts*' --fix",
    "npx": "npx npm-force-resolutions",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --updateSnapshot --watchAll=false",
    "test:coverage": "react-scripts test --coverage  --watchAll=false"
  },
  "peerDependencies": {
    "@navikt/ds-css": "^0.x",
