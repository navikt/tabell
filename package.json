{
  "name": "@navikt/tabell",
  "description": "Tabell",
  "main": "lib/index.cjs",
  "version": "11.0.1",
  "license": "MIT",
  "homepage": ".",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "vite build",
    "dist": "npm run dist:prod",
    "dist:prod": "rm -rf ./lib && webpack --config webpack.prod.cjs",
    "dist:dev": "rm -rf ./lib && webpack --config webpack.dev.cjs",
    "lint": "standardx 'src/*.ts*'",
    "lint:fix": "standardx 'src/*.ts*' --fix",
    "npx": "npx npm-force-resolutions",
    "start": "vite",
    "test": "jest test",
    "test:ci": "jest --updateSnapshot --watchAll=false",
    "test:coverage": "jest --coverage  --watchAll=false"
  },
  "peerDependencies": {
    "@navikt/ds-css": ">=7",
    "@navikt/ds-react": ">=7",
    "classnames": "2",
    "lodash": "4",
    "md5": "2",
    "moment": "2",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "styled-components": ">=5"
  },
  "devDependencies": {
    "@navikt/ds-css": "7.36.0",
    "@navikt/ds-react": "7.40.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/classnames": "2.3.1",
    "@types/jest": "28.1.8",
    "@types/lodash": "4.17.21",
    "@types/md5": "2.3.6",
    "@types/mustache": "4.2.6",
    "@types/node": "22.19.3",
    "@types/prop-types": "15.7.5",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-syntax-highlighter": "15.5.13",
    "@types/styled-components": "5.1.26",
    "@vitejs/plugin-react": "4.3.4",
    "classnames": "2.3.1",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "7.1.2",
    "inspectpack": "4.7.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-styled-components": "7.2.0",
    "md5": "2.3.0",
    "moment": "2.30.1",
    "mustache": "4.2.0",
    "peer-deps-externals-webpack-plugin": "1.0.4",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-normalize": "10.0.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-syntax-highlighter": "16.1.0",
    "standardx": "7.0.0",
    "style-loader": "4.0.0",
    "styled-components": "5.3.5",
    "svg-url-loader": "7.1.1",
    "ts-jest": "29.4.6",
    "ts-loader": "9.3.1",
    "typescript": "4.9.5",
    "url-loader": "4.1.1",
    "vite": "6.4.1",
    "vite-tsconfig-paths": "5.1.4",
    "webpack-cli": "4.10.0",
    "webpack-stats-plugin": "1.1.3"
  },
  "resolutions": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/tabell.git"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "globals": [
      "Blob",
      "File",
      "XMLHttpRequest",
      "HTMLButtonElement",
      "HTMLCanvasElement",
      "HTMLElement",
      "HTMLInputElement",
      "FileReader",
      "ReactDOM",
      "WebSocket",
      "act",
      "fetch",
      "describe",
      "it",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach",
      "expect",
      "shallow",
      "mount",
      "render",
      "renderer",
      "jest",
      "JSX"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "ignore": [
      "build/**/*.js",
      "public/**/*.js",
      "node_modules/**/*.js",
      "build/**/*.tsx",
      "public/**/*.tsx",
      "node_modules/**/*.tsx",
      "src/**/*.snap"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module"
}
